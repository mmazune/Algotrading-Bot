# Copy this to /etc/axfl/axfl.env and edit values, then set perms to 600
OANDA_API_KEY=your_oanda_practice_token
OANDA_ACCOUNT_ID=your_oanda_practice_account_id
OANDA_ENV=practice

# Comma-separated if you have multiple
FINNHUB_API_KEYS=your_finnhub_key_or_keys_comma_separated

# Discord alerts
DISCORD_WEBHOOK_URL=your_discord_webhook_url

# Trading safety & monitoring
AXFL_MIN_UNITS=100
AXFL_ALERTS_ENABLED=1
AXFL_ALERT_SUMMARY_TIME_UTC=16:05

# Debug logging (0 or 1)
AXFL_DEBUG=0
